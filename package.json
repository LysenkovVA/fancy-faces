{
  "name": "fancy-faces",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "db:client:generate:dev": "dotenv -e .env.development -- npm exec prisma generate --schema",
    "db:reset:dev": "dotenv -e .env.development -- npx prisma migrate reset --force --skip-seed",
    "db:push:dev": "npm run db:client:generate:dev && dotenv -e .env.development -- npm exec prisma db push",
    "db:seed:dev": "dotenv -e .env.development -- npx prisma db seed",
    "db:init:dev": "npm run db:client:generate:dev && npm run db:reset:dev && npm run db:push:dev && npm run db:seed:dev",
    "db:client:generate:prod": "dotenv -e .env.production -- npm exec prisma generate --schema",
    "db:reset:prod": "dotenv -e .env.production -- npx prisma migrate reset --force --skip-seed",
    "db:push:prod": "npm run db:client:generate:prod && dotenv -e .env.production -- npm exec prisma db push",
    "db:seed:prod": "dotenv -e .env.production -- npx prisma db seed",
    "db:init:prod": "npm run db:client:generate:prod && npm run db:reset:prod && npm run db:push:prod && npm run db:seed:prod",
    "docker:compose:up:prod": "docker compose -f docker/production/compose.yaml --env-file .env.production up -d",
    "docker:compose:build:prod": "docker compose -f docker/production/compose.yaml --env-file .env.production build"
  },
  "prisma": {
    "schema": "./src/database/prisma",
    "seed": "ts-node ./src/database/seed.ts"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.0.2",
    "@prisma/client": "^6.6.0",
    "@reduxjs/toolkit": "^2.6.1",
    "antd": "^5.24.7",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jose": "^6.0.10",
    "next": "15.3.0",
    "pg": "^8.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-redux": "^9.2.0",
    "react-to-print": "^3.0.6",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20"
  }
}
